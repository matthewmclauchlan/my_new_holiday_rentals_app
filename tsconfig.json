{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
      "strict": true,
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "isolatedModules": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": [
      "app/**/*.ts",
      "app/**/*.tsx",
      "global.d.ts"
    ]
  }
  